{
  "expo": {
    "name": "AlumNIT",
    "slug": "AlumNIT",
    "scheme": "aluminit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.solver.alumnit",
      "supportsTablet": true,
      "infoPlist": {
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.573659707281-0d0f4f3uoo7cn1hrko153lttuasmec13"
            ]
          }
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.solver.alumnit"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.573659707281-0d0f4f3uoo7cn1hrko153lttuasmec13"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "e10cd90a-33ec-4e71-9589-2404420a07ba"
      }
    }
  }
}
